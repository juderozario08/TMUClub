{
    "expo": {
        "name": "DanceClub",
        "slug": "DanceClub",
        "version": "1.0.0",
        "orientation": "portrait",
        "scheme": "DanceClub",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "automatic"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.danceclub",
            "infoPlist": {
                "UIBackgroundModes": [
                    "audio"
                ]
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "automatic"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png",
            "bundler": "metro"
        }
    }
}
